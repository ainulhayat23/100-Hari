 public class Main {
    
    public static void main(String[] args) {
        //Tipe data non Primitif 
        //String = String
        String nama = "Ainul Hayat";
        System.out.println("nama = "+ nama);
        
        String kelas = "Informatika B23";
        System.out.println("kelas = "+ kelas);
        
        String nomor_Hp = "0812-6416-7744";
        System.out.println("nomor_Hp = "+ nomor_Hp);
        
        //Tipe data primitif
        
        //Integer = int
        int umur = 18;
        System.out.println("umur = "+ umur + " tahun");
        
        //Floating pint = float
        float berat = (float)57.9;
        System.out.println("berat = "+ berat + " kg");
        
        //Double floating pint = double
        double tinggi = 165.9;
        System.out.println("tinggi = "+ tinggi + " cm");
        
        //Character = char
        char jenis_kelamin = 'L';
        System.out.println("jenis_kelamin = "+ jenis_kelamin);
        
        //Boolean = boolean
        boolean status_pernikahan = false;
        System.out.println("status_pernikahan = "+ status_pernikahan);
    }
    
}
